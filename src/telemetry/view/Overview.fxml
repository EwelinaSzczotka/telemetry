<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="telemetry.view.OverviewController">
   <children>
      <Button fx:id="connectBtn" layoutX="32.0" layoutY="31.0" mnemonicParsing="false" onAction="#connect" prefHeight="42.0" prefWidth="185.0" text="Connect to server" />
      <Button fx:id="disconnectBtn" disable="true" layoutX="32.0" layoutY="89.0" mnemonicParsing="false" onAction="#disconnect" prefHeight="42.0" prefWidth="185.0" text="Disconnect" />
      <Label fx:id="connectLbl" layoutX="32.0" layoutY="150.0" text="Disconnected.">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
